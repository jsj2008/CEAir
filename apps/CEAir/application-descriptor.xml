<application xmlns="http://www.worklight.com/application-descriptor" id="CEAir" platformVersion="5.0.2">
    <displayName>CEAir</displayName>
    <description>CEAir</description>
    <author>
        <name>Thierry Xing</name>
        <email>thierry.xing@gmail.com</email>
        <homepage>http://www.enwaysoft.com</homepage>
        <copyright>Enwaysoft</copyright>
